#### Common Setup

set REPO_ROOT [exec git rev-parse --show-toplevel]
cd ${REPO_ROOT}

source ${REPO_ROOT}/iceoryx2-testing/end-to-end-testing/common.exp

#### Test Setup

set timeout 10

#### Test

spawn cargo run --bin iox2-config -- show system
set id_iox2_config $spawn_id

#### Test Assertion

expect_output_from $id_iox2_config "posix system configuration"
expect_output_from $id_iox2_config "system info"
expect_output_from $id_iox2_config "limit"
expect_output_from $id_iox2_config "options"
expect_output_from $id_iox2_config "features"
expect_output_from $id_iox2_config "process resource limits"

show_test_passed
