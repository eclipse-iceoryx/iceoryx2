#### Common Setup

set REPO_ROOT [exec git rev-parse --show-toplevel]
cd ${REPO_ROOT}

source ${REPO_ROOT}/iceoryx2-testing/end-to-end-testing/common.exp

#### Test Setup

set timeout 10

spawn cargo run --example event_notifier
set id_notifier $spawn_id

spawn target/ff/cc/build/examples/c/event/example_c_event_listener

#### Test Assertion

expect_output_from $id_notifier "Trigger event with id*"
expect_output "event was triggered with id: 3"

show_test_passed
