#### Common Setup

set REPO_ROOT [exec git rev-parse --show-toplevel]
cd ${REPO_ROOT}

source ${REPO_ROOT}/iceoryx2-testing/end-to-end-testing/common.exp

#### Test Setup

set timeout 10

spawn target/ff/cc/build/examples/c/event_multiplexing/example_c_event_multiplexing_notifier 42 hypnotoad
set id_notifier_1 $spawn_id

spawn target/ff/cc/build/examples/c/event_multiplexing/example_c_event_multiplexing_notifier 13 brainslug
set id_notifier_2 $spawn_id

spawn target/ff/cc/build/examples/c/event_multiplexing/example_c_event_multiplexing_wait hypnotoad brainslug

#### Test Assertion

expect_output_from $id_notifier_1 "Trigger event with id*"
expect_output_from $id_notifier_2 "Trigger event with id*"
expect_output "Received trigger from \"hypnotoad\" :: 42"
expect_output "Received trigger from \"brainslug\" :: 13"

show_test_passed
